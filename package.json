{
  "name": "react18-antd5",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/compatible": "^5.1.0",
    "@ant-design/pro-components": "^2.3.35",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "devAndWatchPublic": "concurrently \"yarn watchPublicDir\" \"yarn start\"",
    "buildAndPack": "yarn build && yarn zipPack",
