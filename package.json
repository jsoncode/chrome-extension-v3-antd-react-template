      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@types/chrome": "^0.0.203",
    "adm-zip": "^0.5.9",
    "concurrently": "^7.6.0",
    "craco-less": "^2.0.0",
    "cross-env": "^7.0.3"
  },
  "homepage": "."
